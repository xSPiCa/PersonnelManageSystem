@model PersonnelManageSystem.Models.ReturnResult

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>请登录！</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
    <link rel="stylesheet" href="~/css/sidebar.css"/>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-table.css">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

</head>
<body>

<div class="container">
    
    <div class="main">
            <form asp-action="LoginWithUser" method="post">
                <div class="form-group">
                    <label for="login-username">username</label>
                    <input name="username" type="text" class="form-control" id="login-username" aria-describedby="emailHelp">
                </div>
                <div class="form-group">
                    <label for="login-password">Password</label>
                    <input name="password" type="password" class="form-control" id="login-password">
                </div>
                <label class="text-danger">@Model?.Message</label>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
    </div>

</div>




</body>


<footer class="border-top footer text-muted">
  
  <div class="container">

  </div>
</footer>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/login.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>